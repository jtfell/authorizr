{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "padded-blocks": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
  }
}
